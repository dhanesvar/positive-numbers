nums = [12,-7,5,64,-14]
print("Original numbers in the list1: ",nums)
Pos_nums = list(filter(lambda x: x >0, nums))
print("Positive numbers in the list: ",Pos_nums)



nums = [12,14,-95,3]
print("Original numbers in the list2: ",nums)
Pos_nums = list(filter(lambda x: x >0, nums))
print("Positive numbers in the list: ",Pos_nums)

